apiVersion: apps/v1
kind: Deployment
metadata:
  labels:
    app: reddep
  name: reddep
spec:
  replicas: 3
  selector:
    matchLabels:
      app: reddep
  strategy: {}
  template:
    metadata:
      labels:
        app: reddep
    spec:
      containers:
      - image: spurin/nginx-red
        name: nginx-red
        ports:
        - containerPort: 80
        resources: {}
status: {}
